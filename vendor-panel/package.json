{
  "name": "vendor-panel",
  "version": "1.0.0",
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "wait": "node ./scripts/wait-for-backend.js",
    "dev": "pnpm run wait && node ./scripts/launch-vendor.js",
    "start": "pnpm run wait && pnpm run build && pnpm exec vite preview --host 0.0.0.0 --port $PORT",
    "build": "vite build",
    "build:preview": "vite build",
    "build:admin": "medusa build",
    "preview": "vite preview",
    "test": "vitest --run",
    "i18n:validate": "node ./scripts/i18n/validate-translation.js",
    "i18n:schema": "node ./scripts/i18n/generate-schema.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/app.js",
  "module": "dist/app.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/app.css",
      "require": "./dist/app.css"
    },
    "./root": "./",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/mercurjs/vendor-panel"
  },
  "files": [
    "package.json",
    "src",
    "index.html",
    "dist"
  ],
  "dependencies": {
    "...": "..."
  },
  "devDependencies": {
    "...": "..."
  }
}
