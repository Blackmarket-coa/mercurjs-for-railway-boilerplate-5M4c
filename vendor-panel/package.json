{
  "name": "vendor-panel",
  "version": "1.0.0",
  "pnpm": {
    "overrides": {
      "axios": ">=1.13.5",
      "glob": ">=10.5.0",
      "qs": ">=6.14.2",
      "@remix-run/router": ">=1.23.2",
      "react-router": ">=6.30.2",
      "@medusajs/framework": "2.12.5",
      "lodash": ">=4.17.23"
    }
  },
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "wait": "echo 'Backend check handled by launch-vendor.js'",
    "dev": "node ./scripts/launch-vendor.js",
    "start": "node ./scripts/launch-vendor.js",
    "build": "vite build && tsup && node ./scripts/generate-types.js",
    "build:preview": "vite build",
    "build:admin": "medusa build",
    "preview": "vite preview",
    "test": "vitest --run",
    "i18n:validate": "node ./scripts/i18n/validate-translation.js",
    "i18n:schema": "node ./scripts/i18n/generate-schema.js",
    "lint": "eslint .  --ext ts,tsx --max-warnings 0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit -p tsconfig.build.json"
  },
  "main": "dist/app.js",
  "module": "dist/app.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/app.css",
      "require": "./dist/app.css"
    },
    "./root": "./",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mercurjs/vendor-panel"
  },
  "files": [
    "package.json",
    "src",
    "index.html",
    "dist"
  ],
  "dependencies": {
    "@ariakit/react": "^0.4.15",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "3.4.2",
    "@medusajs/admin-sdk": "2.12.5",
    "@medusajs/admin-shared": "2.12.5",
    "@medusajs/icons": "2.12.5",
    "@medusajs/js-sdk": "2.12.5",
    "@medusajs/ui": "4.0.28",
    "@tanstack/react-query": "5.64.2",
    "@tanstack/react-table": "8.20.5",
    "@tanstack/react-virtual": "^3.8.3",
    "@uiw/react-json-view": "2.0.0-alpha.39",
    "cmdk": "^0.2.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "i18next": "23.7.11",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "lodash": "^4.17.23",
    "match-sorter": "^6.3.4",
    "medusajs-launch-utils": "^0.0.18",
    "motion": "^11.15.0",
    "qs": "^6.14.2",
    "radix-ui": "1.1.2",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-currency-input-field": "^3.6.11",
    "react-day-picker": "^9.6.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "7.49.1",
    "react-i18next": "13.5.0",
    "react-jwt": "^1.2.0",
    "react-router-dom": "^6.30.2",
    "recharts": "^2.15.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@medusajs/admin-shared": "2.12.5",
    "@medusajs/admin-vite-plugin": "2.12.5",
    "@medusajs/types": "2.12.5",
    "@medusajs/ui-preset": "2.12.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.11.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "4.2.1",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.17",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.4.14",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^3.0.5"
  }
}
