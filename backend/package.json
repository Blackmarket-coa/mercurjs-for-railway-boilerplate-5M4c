{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "ib": "init-backend",
    "init-algolia": "node .medusa/server/src/scripts/init-algolia.js",
    "start": "init-backend && pnpm run init-algolia && cd .medusa/server && medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@bmc/restaurant-marketplace": "file:.yalc/@bmc/restaurant-marketplace",
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/cart": "^2.12.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/core-flows": "^2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/icons": "2.11.3",
    "@medusajs/order": "^2.12.3",
    "@medusajs/product": "^2.12.3",
    "@medusajs/ui": "4.0.27",
    "@medusajs/utils": "^2.11.3",
    "@mercurjs/algolia": "^1.4.5",
    "@mercurjs/b2c-core": "^1.4.5",
    "@mercurjs/commission": "^1.4.5",
    "@mercurjs/framework": "^1.4.5",
    "@mercurjs/payment-stripe-connect": "^1.4.5",
    "@mercurjs/requests": "^1.4.5",
    "@mercurjs/resend": "^1.4.5",
    "@mercurjs/reviews": "^1.4.5",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/knex": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/postgresql": "6.4.16",
    "algoliasearch": "^5.44.0",
    "awilix": "^8.0.1",
    "express": "^4.21.2",
    "json-rpc-2.0": "^1.7.1",
    "medusajs-launch-utils": "^0.0.18",
    "minio": "^8.0.3",
    "multer": "^2.0.2",
    "my-plugin": "file:.yalc/my-plugin",
    "pg": "^8.13.0",
    "qrcode": "^1.5.4",
    "stripe": "^17.7.0",
    "ulid": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@graphql-codegen/visitor-plugin-common": "^6.2.2",
    "@medusajs/medusa": "2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@mikro-orm/cli": "6.4.16",
    "@swc/core": "1.15.7",
    "@swc/jest": "^0.2.36",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.13",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
