{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "DATABASE_URL=postgresql://placeholder:placeholder@localhost:5432/placeholder medusa build",
    "dev": "medusa develop",
    "start": "init-backend && npm run init-algolia && cd .medusa/server && medusa start",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "init-algolia": "node .medusa/server/src/scripts/init-algolia.js",
    "ib": "init-backend"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.11.3",
