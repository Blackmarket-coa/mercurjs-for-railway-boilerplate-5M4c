{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "pnpm": {
    "overrides": {
      "glob": ">=10.5.0",
      "qs": ">=6.14.1",
      "jws": ">=3.2.3",
      "axios": ">=1.13.5",
      "@isaacs/brace-expansion": ">=5.0.1",
      "@remix-run/router": ">=1.23.2",
      "fast-xml-parser": ">=5.3.4",
      "@medusajs/core-flows": "2.12.5",
      "@medusajs/orchestration": "2.12.5",
      "@medusajs/workflows-sdk": "2.12.5"
    }
  },
  "scripts": {
    "postinstall": "node scripts/patch-mercurjs.js || true",
    "build": "medusa build",
    "ib": "init-backend",
    "start": "node scripts/railway-start.js",
    "start:simple": "medusa db:migrate --execute-safe-links && init-backend && cd .medusa/server && medusa start",
    "dev": "medusa develop",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:if-needed": "node scripts/conditional-seed.js",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit --coverage --coverageProvider=v8",
    "test:unit:ci": "npm run test:unit && node ./scripts/ensure-coverage.js"
  },
  "dependencies": {
    "@bmc/restaurant-marketplace": "file:./restaurant-marketplace",
    "@medusajs/admin-sdk": "2.12.5",
    "@medusajs/cart": "2.12.5",
    "@medusajs/cli": "2.12.5",
    "@medusajs/core-flows": "2.12.5",
    "@medusajs/framework": "2.12.5",
    "@medusajs/icons": "2.12.5",
    "@medusajs/js-sdk": "2.12.5",
    "@medusajs/order": "2.12.5",
    "@medusajs/product": "2.12.5",
    "@medusajs/ui": "4.0.28",
    "@medusajs/utils": "2.12.5",
    "@mercurjs/b2c-core": "^1.4.5",
    "@mercurjs/commission": "^1.4.5",
    "@mercurjs/framework": "^1.4.5",
    "@mercurjs/payment-stripe-connect": "^1.4.5",
    "@mercurjs/reviews": "^1.4.5",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/knex": "6.4.16",
    "@mikro-orm/migrations": "6.4.16",
    "@mikro-orm/postgresql": "6.4.16",
    "@react-email/components": "^1.0.3",
    "@stellar/stellar-sdk": "^13.1.0",
    "@tanstack/react-query": "5.64.2",
    "@types/bcryptjs": "^3.0.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "algoliasearch": "^5.44.0",
    "awilix": "^8.0.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "json-rpc-2.0": "^1.7.1",
    "jsonwebtoken": "^9.0.3",
    "medusajs-launch-utils": "^0.0.18",
    "minio": "^8.0.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.13.0",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "resend": "^6.6.0",
    "scrypt-kdf": "^2.0.1",
    "stripe": "^17.7.0",
    "ulid": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@graphql-codegen/visitor-plugin-common": "^6.2.2",
    "@medusajs/medusa": "2.12.5",
    "@medusajs/test-utils": "2.12.5",
    "@mikro-orm/cli": "6.4.16",
    "@swc/core": "1.15.7",
    "@swc/jest": "^0.2.36",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.13",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "bcrypt": "^6.0.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.20.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
